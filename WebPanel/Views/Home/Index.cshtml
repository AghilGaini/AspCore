@model IEnumerable<CityDomain>

@{
    ViewBag.Title = "Index Page";
}

<table class="table table-dark" style="margin-top:5%">
    <thead>
        <tr>
            <th>City Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Name
                </td>
            </tr>
        }
    </tbody>
</table>

