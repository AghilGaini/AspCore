@model List<IdentityRole>
@{
    ViewBag.Title = "Role Page";
}

<table class="table table-dark">
    <thead>
        <tr>
            <th>RoleName</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="Create" asp-controller="Role" class="btn btn-primary">
        Create New Role
    </a>
</div>


